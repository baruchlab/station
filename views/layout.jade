!!!
html
  head
    title= 'Miscommunication Station by Telekommunisten'
    link(rel='stylesheet', href='/stylesheets/style.css')
    script(type='text/javascript', src='/socket.io/socket.io.js')
    script(type='text/javascript', src='/javascripts/station.js')
  body(onload='Station.connect("#{channel}")', onbeforeunload='Station.stop()')
    div#main
      table(cellpadding=0, cellspacing=0)
        tbody
          tr
            td
              img(src='/images/left.gif')
            td
              img(src='/images/top.gif')
              br
              !{body}
              br
              img(src='/images/bottom.gif')
            td
              img(src='/images/right.gif')
            td
              div#transcript
              br
              input#input(autofocus='')
    div#jerrold
      div.corner.tl
        div.a
        div.b
        div.c
      div.corner.tr
        div.a
        div.b
        div.c
      ul.starcom
        li.down
          a.up(href='/bridge/1') [ 1 ]
        li.down
          a.up(href='/bridge/2') [ 2 ]
        li.down
          a.up(href='/bridge/3') [ 3 ]
        li.down
          a.up(href='/bridge/4') [ 4 ]
        li.down
          a.up(href='/bridge/5') [ 5 ]
        li.down
          a.up(href='/bridge/6') [ 6 ]
        li.down
          a.up(href='/bridge/7') [ 7 ]
        li.down
          a.up(href='/bridge/8') [ 8 ]
        li.down
          a.up(href='/') ( X )
      div.corner.bl
        div.a
        div.c
        div.b
      div.corner.br
        div.a
        div.c
        div.b

